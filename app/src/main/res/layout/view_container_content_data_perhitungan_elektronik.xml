<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="gone">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible">
        <!--validate-->
        <EditText
            android:id="@+id/validate_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/validate_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/validate_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--_ktp-->
        <EditText
            android:id="@+id/take_ktp_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_ktp_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_ktp_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--_customer-->
        <EditText
            android:id="@+id/take_customer_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_customer_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_customer_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--_sallary slip-->
        <EditText
            android:id="@+id/take_paycheck_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_paycheck_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_paycheck_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--_addtional documents-->
        <EditText
            android:id="@+id/take_additional_documents_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_additional_documents_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_additional_documents_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <!--_signature-->
        <EditText
            android:id="@+id/take_signature_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_signature_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/take_signature_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--submit-->
        <EditText
            android:id="@+id/submit_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/submit_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/submit_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
        <!--sync-->
        <EditText
            android:id="@+id/sync_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/sync_longitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />

        <EditText
            android:id="@+id/sync_latitude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tvTenorELC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tenor_container" />

            <EditText
                android:id="@+id/et_tenor_product_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:hint="@string/tenor_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flat_rate_container" />

            <EditText
                android:id="@+id/et_flat_rate_perhitungan_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"

                android:focusableInTouchMode="false"
                android:hint="@string/flat_rate_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
