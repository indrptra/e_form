                android:focusableInTouchMode="false"
                android:hint="@string/bunga_pembiayaan_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/total_pinjaman_container" />

            <EditText
                android:id="@+id/et_total_pinjaman_container"
                android:layout_width="match_parent"
