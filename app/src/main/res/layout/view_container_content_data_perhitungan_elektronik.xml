            <EditText
                android:id="@+id/et_dp_perhitungan_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"

                android:focusableInTouchMode="false"
                android:hint="@string/dp_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/biaya_adm_container" />

            <EditText
                android:id="@+id/et_biaya_admin_perhitungan_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"

                android:focusableInTouchMode="false"
                android:hint="@string/biaya_adm_container"
                android:inputType="number"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tipe_pembayaran_container" />

            <EditText
                android:id="@+id/et_tipe_pembayaran_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"

                android:focusableInTouchMode="false"
                android:hint="@string/tipe_pembayaran_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/biaya_lainnya_container" />

            <EditText
                android:id="@+id/et_biaya_lainnya_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_4dp"
                android:focusable="false"

                android:focusableInTouchMode="false"
                android:hint="@string/biaya_lainnya_container"
                android:longClickable="false" />
        </LinearLayout>

        <LinearLayout
